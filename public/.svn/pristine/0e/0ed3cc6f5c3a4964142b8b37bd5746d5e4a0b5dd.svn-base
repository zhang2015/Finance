img{
	display: block;
	margin: auto;
}
.download_wrapper{
	position: absolute;
	width: 100%;
	min-height: 100%;
	height: auto;
    display: -moz-box; 
    display:-webkit-flex;
    display: -ms-flexbox;
    display:flex;
	flex-direction: column;
	justify-content: space-between;
	align-items: center;
	top: 0;
	left: 0;
	background-image: url(../images/bg.png);
	background-position: center;
	background-size: 100% 100%;
	background-repeat: no-repeat;
}
.download_wrapper div{
	/*display: -webkit-box;*/ 
    display: -moz-box; 
    display:-webkit-flex;
    display: -ms-flexbox;
    display:flex;
	flex-direction: column;
	justify-content: flex-start;
	align-items: center;
}
.download_logo{
	width: 4.8rem;
	height: auto;
	padding-top: 3.2rem;
}
.download_icon{
	width: 3.36rem;
	height: auto;
	padding-top: 0.133333rem;
}
.download_tagline{
	width: 4.026666rem;
	height: auto;
	padding-top: 0.533333rem;
}
.download_ios,.download_android{
	width: 5.866666rem;
	height: auto;
	padding-top: 0.533333rem;
}
p{
	padding-bottom: 5px;
	color: #adadad;
	text-align: center;
	padding-top: 10px;
}
#openit {
	/*display: -webkit-box;*/ 
	position: absolute;
    display: -moz-box; 
    display:-webkit-flex;
    display: -ms-flexbox;
    display:flex;
	display: none;
	width: 100%;
	height: 100%;
	justify-content: center;
	align-items: center;
	background: rgba(0, 0, 0, 0.85);
	color: #FFFFFF;
	top: 0;
	left: 0;
	z-index: 9;
}
#openit p{
	font-size: 25px;
	line-height: 30px;
}
#openit img {
	position: absolute;
	right: 0;
	top: 0;
	width: 1.25rem;
	height: auto;
}